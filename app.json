{
  "expo": {
    "name": "AshtavinayakApp",
    "slug": "AshtavinayakApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/Bappa.png",
    "scheme": "myapp",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/Bappa.png",
      "resizeMode": "contain",
      "backgroundColor": "#FCF1DF"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/Bappa.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.punuuu03.AshtavinayakApp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/Bappa.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/Bappa.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "e00d35d0-6382-4d06-9736-3f886827218f"
      }
    },
    "owner": "parthsalunkhe"
  }
}
